{
  "name": "smartweb-design",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "start": "npm run dev",
    "start-netlify": "ntl dev",
    "build": "rimraf dist && npm run lint && tsc && vite build",
    "preview": "vite preview",
    "lint-fix": "biome lint . --apply",
    "lint": "biome lint .",
    "tsc-init": "tsc --init",
    "tsc": "tsc --listEmittedFiles",
    "test-sendgrid": "ts-node netlify/sendgrid/hello-world.ts",
    "compress-image": "cwebp -q 70 public/images/backgrounds/landing-page.jpg -o public/images/backgrounds/landing-page.webp",
    "migrate-eslint-to-biome": "biome migrate eslint --write",
    "migrate-prettier-to-biome": "biome migrate prettier --write",
    "csv2json-dates": "csv2json src/data/google-search/Dates.csv -o src/data/google-search/Dates.json"
  },
  "dependencies": {
    "@netlify/functions": "2.6.0",
    "@sendgrid/mail": "8.1.1",
    "@types/intro.js": "5.1.5",
    "@types/netlify-identity-widget": "1.9.6",
    "@types/node": "20.11.28",
    "bootstrap": "5.3.3",
    "bootstrap-icons": "1.11.3",
    "intro.js": "7.2.0",
    "netlify-identity-widget": "1.9.2",
    "react": "18.2.0",
    "react-bootstrap": "2.10.2",
    "react-dom": "18.2.0",
    "react-player": "2.15.1",
    "react-router-dom": "6.22.3",
    "recharts": "2.12.6",
    "use-local-storage-state": "19.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.1",
    "@mrodrig/json-2-csv-cli": "5.0.0",
    "@types/grecaptcha": "3.0.8",
    "@types/react": "18.2.67",
    "@types/react-dom": "18.2.22",
    "@vitejs/plugin-react": "4.2.1",
    "netlify-cli": "17.19.5",
    "rimraf": "5.0.5",
    "ts-node": "10.9.2",
    "typescript": "5.4.2",
    "vite": "5.1.6"
  }
}
